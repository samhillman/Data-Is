{
  "manifest_version": 2,
  "name": "DataIs",
  "description": "Replaces the phrase 'data are' with 'data is'.",
  "homepage_url": "https://github.com/samhillman/",
  "version": "1.0",
  "icons": {
    "48": "icons/replace-48.png",
    "96": "icons/replace-96.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./replace.js"]
    }
  ]
}